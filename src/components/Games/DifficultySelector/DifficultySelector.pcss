.selectorHeader{
  @apply text-center;
}

.difficultySelector {
  @apply flex justify-center;
}

.selectorBtnWrap {
  @apply w-16 border-b m-2 uppercase rounded-full bg-sky-700 text-sky-200 font-medium;
  &:hover {
    @apply bg-sky-200 text-sky-900;
  } 

  &:active {
    @apply bg-sky-900 text-sky-200;
  }

  .btn {
    @apply w-full h-full p-2;
  }
}