.footer {
  @apply container mx-auto h-[60px] flex items-center justify-between p-5 bg-sky-800 text-sky-200 rounded-t-md;
}

.school-logo {
  @apply w-24;
  filter: invert(84%) sepia(21%) saturate(688%) hue-rotate(176deg) brightness(103%) contrast(98%);
}
.git-link {
  @apply flex items-center justify-between gap-x-1;
}
.git-link a {
  text-decoration: none;
}
.git-link img {
  @apply w-10;
}
